{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "FluentMigrator.DotNet.Cli/**",
            "FluentMigrator.Console/**",
            "FluentMigrator.MSBuild/**"
          ]
        }
      ],
      "dest": "api",
      "filter": "api-filter.yml",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "net461"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "FluentMigrator documentation",
      "_appFooter": "<span>Copyright Â© 2018 Fluent Migrator Project<br>Generated by <strong>DocFX</strong></span>",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/fluentmigrator/fluentmigrator",
        "branch": "develop",
        "apiSpecFolder": "docfx_project/apidoc"
      }
    }
  }
}